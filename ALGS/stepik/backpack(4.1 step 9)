import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();//кол-во предметов
        int W = scanner.nextInt();// вместимость рюкзака
        double[][] price = new double[n][3];
        double answer = 0;
        double[] tmpSort = new double[3];
        
        for (int i = 0; i < n; ++i) {
            price[i][0] = scanner.nextInt();//цена за весь
            price[i][1] = scanner.nextInt();//всего вес
            if (price[i][1] != 0) {
                price[i][2] = price[i][0] / price[i][1];//цена за кг
            }else {
                price[i][2] = 0;
            }
        }

        for (int i = 0; i < n; i++) {
            for (int j = i + 1; j < n; j++) {
                if (price[j][2] > price[i][2]) {

                    tmpSort[0] = price[i][0];
                    tmpSort[1] = price[i][1];
                    tmpSort[2] = price[i][2];

                    price[i][0] = price[j][0];
                    price[i][1] = price[j][1];
                    price[i][2] = price[j][2];

                    price[j][0] = tmpSort[0];
                    price[j][1] = tmpSort[1];
                    price[j][2] = tmpSort[2];
                }
            }
        }

        for(int i = 0; i < n; ++i){
            if(price[i][1] > W){
                answer += W * price[i][2];
                W = 0;
            }else{
                answer += price[i][1] * price[i][2];
                W-=price[i][1];
            }

            if(W == 0){
                break;
            }
        }
      
        System.out.printf("%.3f", answer);
    }
}
